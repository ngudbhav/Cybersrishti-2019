<?xml version='1.0' encoding='utf-8'?>
<widget id="tech.cybersrishti.cs2019" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Cybersrishti-2019</name>
    <description>
        Hello World sample application that responds to the deviceready event.
    </description>
    <author email="ngudbhav05@hotmail.com" href="https://www.ngudbhav.me">
        NGUdbhav
    </author>
    <content src="index.html" />
    <allow-navigation href="http://*/*" />
    <allow-navigation href="https://*/*" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="14" />
    <preference name="fullscreen" value="true" />
</widget>
